public class areaofcirlce {
    public static void main(String[] args) {
      int r;
      double pi=3.14,area;
      Scanner s=new Scanner(System.in);
      r=s.nextInt();
      System.out.println("enter radius of circle");
      area=pi*r*r;
      System.out.println("Area of circle: "+area);
    }

}
